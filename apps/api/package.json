{
  "name": "api",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run db:push && tsx watch src/server.ts",
    "build": "npm run db:generate && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "npm run db:push && vitest run --passWithNoTests",
    "typecheck": "npm run db:generate && tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "db:prepare:schema": "node ./scripts/prepare-prisma-schema.mjs",
    "db:generate": "npm run db:prepare:schema && prisma generate",
    "db:push": "npm run db:prepare:schema && prisma db push",
    "db:migrate": "npm run db:prepare:schema && prisma migrate dev",
    "db:deploy": "npm run db:prepare:schema && prisma migrate deploy",
    "db:studio": "npm run db:prepare:schema && prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.0.0",
    "@prisma/adapter-better-sqlite3": "^7.4.1",
    "@prisma/adapter-mariadb": "^7.4.1",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.2",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.0",
    "mariadb": "^3.5.1",
    "pg": "^8.18.0",
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^7.4.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.19.11",
    "dotenv": "^16.6.1",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}
